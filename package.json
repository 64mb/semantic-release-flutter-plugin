{
  "name": "semantic-release-flutter-plugin",
  "version": "1.1.0",
  "description": "semantic-release plugin to bump pubspec.yaml version with version code support",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist/**/*.js",
    "README.md"
  ],
  "scripts": {
    "build": "ncc build src/index.ts -m",
    "format": "prettier --write .",
    "prepare": "husky install",
    "test": "uvu -r tsm src/__tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PalmDevs/semantic-release-flutter-plugin.git"
  },
  "keywords": [
    "flutter",
    "dart",
    "semantic-release"
  ],
  "author": "PalmDevs",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/PalmDevs/semantic-release-flutter-plugin/issues"
  },
  "homepage": "https://github.com/PalmDevs/semantic-release-flutter-plugin#readme",
  "devDependencies": {
    "@saithodev/semantic-release-backmerge": "^3.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/strictest": "^2.0.1",
    "@types/semantic-release": "^20.0.1",
    "@types/semver": "^7.3.13",
    "@vercel/ncc": "^0.36.1",
    "expect": "^29.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "tsm": "^2.3.0",
    "typescript": "^5.0.4",
    "uvu": "^0.5.6"
  },
  "dependencies": {
    "semantic-release": "^21.0.1",
    "semver": "^7.5.0",
    "yaml": "^2.2.1"
  }
}
